---
permalink: "/"
layout: layout.liquid

renderData:
    title: "Portfolio"

---
<div class="corkboard-top">
<br>
<br>
</div> 
<div class="child corkboard columns is-multiline"> 
    {%- for portfolio-item in contentful-photos -%}
    <div class="item column is-one-third ">
       
        <div class="flip-card">
		<div class=" flip-card-inner">
       		 <div class="flip-card-front">
            
                	<div class=" image is-1by1">
                        <img class="polaroid" src="https:{{portfolio-item.thumbnail.fields.file.url}}?w=400&fm=jpg&fl=progressive&q=80&fit=fill&f=faces" alt="Photo from {{ portfolio-item.itemTitle }}" loading="lazy">
               		 </div>
		
            	<img class="polaroid_icon" src="css/icon_only.png">
                <!--<p class="title is-4 has-text-dark"></p>-->
        	</div>
        	
			<div class="flip-card-back polaroid-back has-text-centered is-flex is-align-items-center">
		
				<div class="polaroid-back-text">
					<a href="{{portfolio-item.slug}}"><p>#{{portfolio-item.itemTitle}}</p></a>
				</div>
		
			</div>
			
		</div>
	</div>
    </div>
    {%- endfor -%}
</div>
<div class="corkboard-bottom">
<br>
<br>
</div>

